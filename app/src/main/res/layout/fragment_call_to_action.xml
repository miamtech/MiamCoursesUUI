<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_call_to_action"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ai.mealz.sdk.components.mealPlanner.callToAction.MealPlannerCallToAction
                android:id="@+id/CallToActionU"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               />

            <ai.mealz.sdk.components.recipeJourney.RecipeJourney
                android:id="@+id/Recipe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/CallToActionU"

                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>